{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/uploads/(.*)",
      "destination": "/server/uploads/$1"
    },
    {
      "source": "/widgets/(.*)",
      "destination": "/public/widgets/$1"
    },
    {
      "source": "/player.html",
      "destination": "/public/player.html"
    }
  ]
}