{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/uploads/(.*)",
      "destination": "/server/uploads/$1"
    },
    {
      "source": "/widgets/(.*)",
      "destination": "/public/widgets/$1"
    },
    {
      "source": "/player.html",
      "destination": "/public/player.html"
    }
  ]
}